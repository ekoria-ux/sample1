<% provide(:title, "新規登録") %>

<%= render "users/shared/logotext" %>

<%= form_with(model: resource, scope: resource_name, url: registration_path(resource_name)) do |f| %>
  <%= render "users/shared/error_messages", resource: resource %>
  <%= render "users/shared/form", f: f, resource: resource %>

  <div class="actions">
    <%= f.submit "メールを認証する", class: "btn btn-dark col-3 mb-3 mx-auto" %>
  </div>
<% end %>